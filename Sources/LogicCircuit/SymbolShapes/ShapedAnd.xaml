<Grid
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Background="Transparent"
>
	<Grid.RowDefinitions>
		<RowDefinition Height="*"/>
		<RowDefinition Height="Auto"/>
		<RowDefinition Height="*"/>
	</Grid.RowDefinitions>

	<Rectangle Grid.Row="0" Width="1" VerticalAlignment="Stretch" HorizontalAlignment="Left" Stroke="Black" Margin="0,10,0,0"/>

	<Path Grid.Column="1" Stroke="Black" StrokeThickness="1" Fill="White" VerticalAlignment="Top" HorizontalAlignment="Left">
		<Path.Data>
			<PathGeometry>
				<PathFigure StartPoint="0,0" IsClosed="True">
					<PathFigure.Segments>
						<LineSegment Point="23,0"/>
						<ArcSegment Size="30,30" Point="23,60" SweepDirection="Clockwise" IsLargeArc="False" RotationAngle="0"/>
						<LineSegment Point="0,60"/>
					</PathFigure.Segments>
				</PathFigure>
			</PathGeometry>
		</Path.Data>
	</Path>


	<Rectangle Grid.Row="2" Width="1" VerticalAlignment="Stretch" HorizontalAlignment="Left" Fill="Black" Margin="0,0,0,10"/>
</Grid>

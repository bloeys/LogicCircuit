<Grid
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
	<Grid.RowDefinitions>
		<RowDefinition Height="Auto"/>
		<RowDefinition Height="Auto"/>
		<RowDefinition Height="Auto"/>
	</Grid.RowDefinitions>

	<Rectangle Grid.Row="0" Name="topLine" Width="1" VerticalAlignment="Stretch" HorizontalAlignment="Left" Fill="Black" Margin="0,10,0,0"/>

	<Path Grid.Column="1" Stroke="Black" StrokeThickness="1" Fill="White" VerticalAlignment="Top" HorizontalAlignment="Left">
		<Path.Data>
			<PathGeometry>
				<PathGeometry.Figures>
					<PathFigureCollection>
						<PathFigure StartPoint="1,13" IsClosed="False" IsFilled="False">
							<PathFigure.Segments>
								<ArcSegment Size="60,60" Point="1,60" SweepDirection="Clockwise" IsLargeArc="False" RotationAngle="0" IsSmoothJoin="True"/>
							</PathFigure.Segments>
						</PathFigure>
						<PathFigure StartPoint="54,36" IsClosed="True">
							<PathFigure.Segments>
								<ArcSegment Size="60,60" Point="5,60" SweepDirection="Clockwise" IsLargeArc="False" RotationAngle="0"/>
								<ArcSegment Size="60,60" Point="5,13" SweepDirection="Counterclockwise" IsLargeArc="False" RotationAngle="0"/>
								<ArcSegment Size="60,60" Point="54,36" SweepDirection="Clockwise" IsLargeArc="False" RotationAngle="0"/>
							</PathFigure.Segments>
						</PathFigure>
					</PathFigureCollection>
				</PathGeometry.Figures>
			</PathGeometry>
		</Path.Data>
	</Path>

	<Rectangle Grid.Row="2" Name="bottomLine" Width="1" VerticalAlignment="Stretch" HorizontalAlignment="Left" Fill="Black" Margin="0,0,0,10"/>
</Grid>
